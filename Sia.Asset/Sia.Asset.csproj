    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <PackageId>Sia.Asset</PackageId>
    <PackageDescription>Asset module for Sia.NET</PackageDescription>
    <Author>Phlamcenth Sicusa</Author>
    <Company>Lunditöph Siagö</Company>
    <RepositoryUrl>https://github.com/lunditoph-siago/Sia.Asset</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Sia" Version="1.9.8" />
    <PackageReference Include="Sia.Reactors" Version="1.9.8" />
    <PackageReference Include="Sia.CodeGenerators" Version="1.9.8" />
  </ItemGroup>

</Project>
